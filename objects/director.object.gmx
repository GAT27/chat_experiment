    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///SENDER

if mouse_check_button_pressed(mb_left)
{   clicked = instance_position(mouse_x,mouse_y,all);
    if clicked == bubble_connect.id
    {   if !show_host
        {   QCLOUD(1);
            with bubble_connect instance_destroy();
            with bubble_sound instance_destroy();
        }
        else
        {   QCLOUD(2);
            instance_create(480,250,bubble_connect);
            instance_create(480,350,bubble_sound);
        }
    }
    else if instance_exists(bubble_host) and clicked.object_index==bubble_host
    {   QCLOUD(clicked);
        with bubble_host instance_destroy();
        //show_host = 0;
    }
    else if clicked == bubble_sound.id
    {   if rec
        {   rec = 1;
            audio_stop_recording(channel);
            show_debug_message("VOICE OFF");
        }
        else
        {   rec = burst + 1;
            channel = audio_start_recording(0);
            show_debug_message("VOICE ON");
        }
    }
}

rec = clamp(rec-1,0,rec);
if rec == burst
{   buffer_resize(buffer_pak,0);
    full_l = 0;
    //channel = audio_start_recording(0);
}
if (rec==2) or (rec==1)
{   //audio_stop_recording(channel);
    if online
    {   network_send_udp(peer,ipp[0],ipp[1],abuff,buffer_tell(abuff));
    }
    else
    {   audio_free_buffer_sound(noise);
        buffer_delete(abuff);
        abuff = buffer_create(1,buffer_grow,1);
        buffer_copy(buffer_pak,0,full_l,abuff,0);
        noise = audio_create_buffer_sound(abuff,buffer_s16,16000,0,full_l,audio_3d);
        ns = audio_play_sound(noise,2,false);
        audio_sound_pitch(ns,(burst-2) / (burst-3));
        //show_debug_message("bytes: " + string(full_l));
        //show_debug_message("pitch: " + string(audio_sound_get_pitch(noise)));
        //show_debug_message("gain: " + string(audio_sound_get_gain(noise)));
    }
    if rec == 2
        rec = burst + 1;
    else
        rec--;
}

if online and buffer_get_size(vbuff)==1
{   buffer_copy(buffer_pak,0,voice_l,vbuff,0);
    noise = audio_create_buffer_sound(vbuff,buffer_s16,16000,0,voice_l,audio_3d);
    audio_play_sound(noise,2,false);
}

</string>
          </argument>
        </arguments>
      </action>
